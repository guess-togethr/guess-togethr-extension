{
  "name": "guess-togethr",
  "version": "0.0.1",
  "description": "A web extension that lets you play GeoGuessr with friends",
  "private": "true",
  "scripts": {
    "build": "cross-env NODE_ENV=production node -r ts-node/register utils/build.js",
    "start": "cross-env NODE_ENV=development node -r ts-node/register utils/webserver.js",
    "start:server": "./node_modules/.bin/discovery-swarm-webrtc",
    "debug": "node --lazy --inspect-brk=9229 --preserve-symlinks --preserve-symlinks-main utils/webserver.js",
    "cosmos": "node -r ts-node/register ./node_modules/react-cosmos/bin/cosmos",
    "devtools": "concurrently \"yarn react-devtools\" \"yarn remotedev --port 3030\""
  },
